{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"strictNullChecks": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
			"site": ["site.config.ts"]
		},
		"types": ["astro/client", "vite-plugin-pwa/vanillajs", "vite-plugin-pwa/info", "vite-plugin-pwa/pwa-assets"]
	},
	"exclude": ["node_modules", "dist", "public"]
}
